// Funkce umocňuje základ požadovanným exponentem
function pow(base, exp) {
    let result = 1;
    for(let i = 0; i < exp; i = i +1) {
        result = result * base;
    }

    return result;
}

// BRUUUH function
function printFib(n) {
     if (n < 1) {
        return 0;
    }
  
    let prev1 = 1;
    let prev2 = 0;

    for (let i = 1; i <= n; i = i + 1) {
          
        if (i > 2) {
            let curr = prev1 + prev2;
            prev2 = prev1;
            prev1 = curr;
            print(curr);
        } else { 
            // TODO: Dodělat do jazyka elseif :Dd 
            if (i == 1) {
                print(prev1);
            } else {
                if (i == 2) {
                    print(prev2);
                }
            }
          
        }
        
    }
}
 

function factorial(n) {
   let fact = 1;

    for (let i = 1; i <= n; i = i + 1) {
        fact = fact * i;
    }

    return fact;
}

/*
Komentář test
base = základ
fact1 = faktorial kolika?
exp = exponent
*/
function combineFunction(base, fact1, exp) {
    let fr1 = pow(base, exp);
    let fr2 = factorial(fact1);

    return fr1 * fr2;
}

